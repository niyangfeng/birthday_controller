.container{width: 100%;height: auto}
.search{width: 100%;height: auto;background: #76EEC6;position: relative;padding: 10rpx;}
.search input{width: 85%;height: 30rpx;font-size: 30rpx;border:1rpx solid #76EEC6;border-radius: 44rpx;margin: 10rpx auto;text-indent: 20rpx;background: #d0f7ea;color: #fff;}
.noteList{width: 100%;height: auto;border-radius: 10rpx;background: #fff;}
.birthdayList_li{width: 100%;height: 130rpx;line-height: 140rpx;padding: 0 30rpx;box-sizing: border-box;position: relative;overflow: hidden;border-bottom: 1rpx solid #d0f7ea}
.birthdayList image{width: 70rpx;height: 70rpx;border-radius: 50%;display: inline-block;background-repeat: no-repeat;background-size: 100% 100%;position: relative;top: 30rpx;float: left;}
.birthdayList_li .name{font-size: 28rpx;color: #333;float: left;margin-left: 20rpx;width: calc(100% - 320rpx);overflow: hidden;text-overflow:ellipsis;white-space: nowrap;}
.birthdayList_li .birthdate{font-size: 22rpx;color: #666;float: right;width: 190rpx;text-align: right;}
.add{width: 80rpx;height: 80rpx;border-radius: 45rpx;background: #76EEC6;color: #fff;line-height: 70rpx;text-align: center;font-size: 80rpx;position: fixed;bottom: 50rpx;left: calc(50% - 40rpx)}